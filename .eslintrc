{
  "root": true,
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "dot-notation": 0,
    "max-len": [2, 100, 2, {
      "ignoreUrls": true,
      "ignoreComments": false,
      "ignorePattern": "^\\s*<(path|polygon)\\s"
    }],
    "quote-props": [2, "as-needed", { "unnecessary": false }],
    "react/no-set-state": 2,
    "react/jsx-boolean-value": [2, "always"]
  }
}
